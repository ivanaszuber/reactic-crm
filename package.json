{
  "name": "reactic-crm",
  "version": "0.0.0",
  "description": "React CRM app",
  "main": "src/app.js",
  "dependencies": {
    "react": "~0.13",
    "classnames": "^2.1.2",
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react-router": "^1.0.0-beta3"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "jest-cli": "^0.4.3",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d src/app.js",
    "build": "browserify . -t babelify | [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js",
    "test": "jest"
  },
  "author": "Ivana Zuber",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "jest": {
    "rootDir": "./src"
  }
}
